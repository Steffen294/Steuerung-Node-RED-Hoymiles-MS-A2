    {
        "id": "648e293a7fdd1c49",
        "type": "function",
        "z": "cdda9bfb14be7999",
        "g": "3822750584f4e06d",
        "name": "Anzeige",
        "func": "function mapBatteryStatusToGerman(status) {\n    switch (status) {\n        case \"discharge\":\n            return \"Speicher L1 - Entladen\";\n        case \"standby\":\n            return \"Speicher L1 - Ruhezustand\";\n        case \"charge\":\n            return \"Speicher L1 - Laden\";\n        default:\n            return status;\n    }\n}\n\nfunction mapGermanToColor(text) {\n    switch (text) {\n        case \"Speicher L1 - Entladen\":\n            return \"red\";\n        case \"Speicher L1 - Laden\":\n            return \"green\";\n        case \"Speicher L1 - Ruhezustand\":\n            return \"orange\";\n        default:\n            return \"\";\n    }\n}\n\nfunction mapGermanToIcon(text) {\n    switch (text) {\n        case \"Speicher L1 - Laden\":\n            return \"autorenew\";\n        case \"Speicher L1 - Entladen\":\n            return \"cached\";\n        case \"Speicher L1 - Ruhezustand\":\n            return \"pause_circle_outline\";\n        default:\n            return \"\";\n    }\n}\n\nmsg.payload = msg.payload.bat_sts;\n\nconst germanStatus = mapBatteryStatusToGerman(msg.payload);\nmsg.payload2 = germanStatus;\n\nmsg.payload = mapGermanToColor(germanStatus);\nmsg.payload3 = mapGermanToIcon(germanStatus);\n\nreturn msg;",
